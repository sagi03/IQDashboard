@{
    ViewBag.Title = "Internal Quality Dashboard";
}
@section Scripts{
    <script type="text/javascript">google.charts.load('current', { packages: ['corechart'] });</script>
    <script src="~/js/dashboard.js"></script>
}
<div class="app-title">
    <div>
        <h1 class="page-header">Dashboard</h1>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
           
            <div class="panel-body">
                <div class="col-md-6">
                    <div class="tile">
                        <div class="tile-title-w-btn">
                            <h3 class="tile-title">Financial Services</h3>
                        </div>
                        <div class="tile-body">
                            <div id="fs" class="embed-responsive-item"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="tile">
                        <div class="tile-title-w-btn">
                            <h3 class="tile-title">Communication Engineering</h3>
                        </div>
                        <div class="tile-body">
                            <div id="ce" class="embed-responsive-item"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="tile">
                        <div class="tile-title-w-btn">
                            <h3 class="tile-title">Life Science</h3>
                        </div>
                        <div class="tile-body">
                            <div id="ls" class="embed-responsive-item"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="tile">
                        <div class="tile-title-w-btn">
                            <h3 class="tile-title">Business Operations</h3>
                        </div>
                        <div class="tile-body">
                            <div id="bo" class="embed-responsive-item"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>